        "def clean_text(text):\n",
        "    if text is None:\n",
        "        return \"\"\n",
        "    text = text.lower()\n",
        "    text = re.sub(r'[^a-z\\s]', '',text)       #to keep only letters and spaces\n",
        "    text = re.sub(r'\\s+', ' ',text).strip()    #removing extra spaces\n",
        "    return text"
      ],
      "metadata": {
